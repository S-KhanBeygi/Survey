{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Hosts": {
    "Api": "https://localhost:5001",
    "dashboard": "http://localhost:4200/"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServer": "Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=DaraSurvey;Data Source=."
  },
  "IdentitySettings": {
    "Registration": {
      "RegisterByUsernameEnabled": false,
      "RegisterByEmailEnabled": false,
      "RegisterByPhoneNumberEnabled": false,
      "RegisterByOtpEnabled": true
    },
    "Password": {
      "RequiredLength": 6,
      "RequiredUniqueChars": 1,
      "RequireNonAlphanumeric": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireDigit": false
    },
    "SignIn": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false
    },
    "Lockout": {
      "AllowedForNewUsers": true,
      "MaxFailedAccessAttempts": 5,
      "DefaultLockoutTimeSpan": "00:05:00"
    },
    "JWT": {
      "AccessTokenExpireMins": 10000080,
      "SecurityKey": "*ju3t@m0m3nt%ARmanemn4Rd345haf3m@#%%^^&&***yyfg++*(.g(_"
    },
    "FileContainers": [
      {
        "Name": "Temp"
      },
      {
        "Name": "Users",
        "Buckets": [ "Image" ]
      },
      {
        "Name": "Survey",
        "Buckets": [ "Logo" ]
      }
    ]
  }
}
